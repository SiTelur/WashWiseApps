                            android:background="@drawable/div_biru" />

                        <View
                            android:id="@+id/divider3"
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:layout_below="@+id/textView42"
                            android:layout_marginTop="16dp"
                            android:background="@drawable/div_biru" />

