            android:layout_below="@+id/textView33"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="16dp"
            app:cardBackgroundColor="@color/biru"
            app:cardCornerRadius="20dp">

            <RelativeLayout
